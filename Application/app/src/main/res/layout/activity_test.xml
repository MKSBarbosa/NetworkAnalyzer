<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TestActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/background_title"
            android:layout_width="428dp"
            android:layout_height="66dp"
            android:background="#243591"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/background_radio"
            android:layout_width="335dp"
            android:layout_height="154dp"
            android:layout_marginTop="24dp"
            android:background="#243591"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/background_title"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/background_rede"
            android:layout_width="332dp"
            android:layout_height="105dp"
            android:layout_marginTop="24dp"
            android:background="#243591"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.506"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/background_radio"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/background_stream"
            android:layout_width="341dp"
            android:layout_height="167dp"
            android:layout_marginTop="24dp"
            android:background="#243591"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.441"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/background_rede"
            app:layout_constraintVertical_bias="0.066" />

        <Button
            android:id="@+id/bt_stop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginBottom="32dp"
            android:text="Stop"
            android:textColorHighlight="#243591"
            android:textColorLink="#CA1818"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/background_stream"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/RSRQ_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="RSRQ"
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/toolbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.297"
            app:layout_constraintStart_toEndOf="@+id/RSRP_name"
            app:layout_constraintTop_toBottomOf="@+id/title_radio" />

        <TextView
            android:id="@+id/title_radio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="Radio Info"
            android:textColor="#ECE2E2"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/toolbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/background_radio" />

        <TextView
            android:id="@+id/Loadtime1_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Load Time:"
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/background_stream"
            app:layout_constraintEnd_toEndOf="@+id/background_stream"
            app:layout_constraintHorizontal_bias="0.061"
            app:layout_constraintStart_toEndOf="@+id/videoView1"
            app:layout_constraintTop_toBottomOf="@+id/Vazao1_name"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/Upload_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="27dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="Upload"
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/toolbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.369"
            app:layout_constraintStart_toEndOf="@+id/Download_name"
            app:layout_constraintTop_toBottomOf="@+id/title_rede" />

        <TextView
            android:id="@+id/title_stream"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="Stream"
            android:textColor="#ECE2E2"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/toolbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/background_stream" />

        <TextView
            android:id="@+id/counter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="initiating ..."
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/background_stream"
            app:layout_constraintEnd_toEndOf="@+id/background_stream"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/title_stream"
            app:layout_constraintVertical_bias="0.181" />

        <TextView
            android:id="@+id/title_rede"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="Network Info"
            android:textColor="#ECE2E2"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/background_rede" />

        <TextView
            android:id="@+id/Ping_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="RTT"
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/background_rede"
            app:layout_constraintEnd_toStartOf="@+id/Download_name"
            app:layout_constraintHorizontal_bias="0.406"
            app:layout_constraintStart_toStartOf="@+id/background_rede"
            app:layout_constraintTop_toBottomOf="@+id/title_rede"
            app:layout_constraintVertical_bias="0.339" />

        <TextView
            android:id="@+id/Vazao1_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="Data Rate: "
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/background_stream"
            app:layout_constraintEnd_toEndOf="@+id/background_stream"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/videoView1"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/RSRP_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="8dp"
            android:text="RSRP"
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/background_radio"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.006"
            app:layout_constraintStart_toStartOf="@+id/background_radio"
            app:layout_constraintTop_toBottomOf="@+id/title_radio"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/SNR_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="30dp"
            android:text="SNR"
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/background_radio"
            app:layout_constraintEnd_toEndOf="@+id/background_radio"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/RSRQ_name"
            app:layout_constraintTop_toBottomOf="@+id/title_radio"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/Download_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Download"
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/background_rede"
            app:layout_constraintEnd_toEndOf="@+id/background_rede"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="@+id/background_rede"
            app:layout_constraintTop_toBottomOf="@+id/title_rede"
            app:layout_constraintVertical_bias="0.339" />

        <TextView
            android:id="@+id/AppName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Network Analyzer"
            android:textColor="#ECE2E2"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/background_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="@+id/background_title"
            app:layout_constraintTop_toTopOf="@+id/background_title" />

        <TextView
            android:id="@+id/RSRP_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="34dp"
            android:layout_marginTop="12dp"
            android:text="0"
            android:textColor="#ECE2E2"
            app:layout_constraintBottom_toBottomOf="@+id/background_radio"
            app:layout_constraintEnd_toEndOf="@+id/background_radio"
            app:layout_constraintHorizontal_bias="0.047"
            app:layout_constraintStart_toStartOf="@+id/background_radio"
            app:layout_constraintTop_toBottomOf="@+id/RSRP_name"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/Ping_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="0"
            android:textColor="#ECE2E2"
            app:layout_constraintBottom_toBottomOf="@+id/background_rede"
            app:layout_constraintEnd_toEndOf="@+id/background_rede"
            app:layout_constraintHorizontal_bias="0.163"
            app:layout_constraintStart_toStartOf="@+id/background_rede"
            app:layout_constraintTop_toBottomOf="@+id/Ping_name"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/Download_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="#ECE2E2"
            app:layout_constraintBottom_toBottomOf="@+id/background_rede"
            app:layout_constraintEnd_toEndOf="@+id/background_rede"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="@+id/background_rede"
            app:layout_constraintTop_toBottomOf="@+id/Download_name"
            app:layout_constraintVertical_bias="0.6" />

        <TextView
            android:id="@+id/Upload_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="#ECE2E2"
            app:layout_constraintBottom_toBottomOf="@+id/background_rede"
            app:layout_constraintEnd_toEndOf="@+id/background_rede"
            app:layout_constraintHorizontal_bias="0.836"
            app:layout_constraintStart_toStartOf="@+id/background_rede"
            app:layout_constraintTop_toBottomOf="@+id/Upload_name"
            app:layout_constraintVertical_bias="0.388" />


        <TextView
            android:id="@+id/Vazao1_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="#ECE2E2"
            app:layout_constraintBottom_toBottomOf="@+id/background_stream"
            app:layout_constraintEnd_toEndOf="@+id/background_stream"
            app:layout_constraintHorizontal_bias="0.366"
            app:layout_constraintStart_toEndOf="@+id/Vazao1_name"
            app:layout_constraintTop_toBottomOf="@+id/counter"
            app:layout_constraintVertical_bias="0.042" />

        <TextView
            android:id="@+id/Loadtime1_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="#ECE2E2"
            app:layout_constraintBottom_toBottomOf="@+id/background_stream"
            app:layout_constraintEnd_toEndOf="@+id/background_stream"
            app:layout_constraintHorizontal_bias="0.338"
            app:layout_constraintStart_toEndOf="@+id/Loadtime1_name"
            app:layout_constraintTop_toBottomOf="@+id/Vazao1_data"
            app:layout_constraintVertical_bias="0.061" />

        <TextView
            android:id="@+id/RSRQ_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="34dp"
            android:layout_marginTop="12dp"
            android:text="0"
            android:textColor="#ECE2E2"
            app:layout_constraintBottom_toBottomOf="@+id/background_radio"
            app:layout_constraintEnd_toEndOf="@+id/background_radio"
            app:layout_constraintHorizontal_bias="0.44"
            app:layout_constraintStart_toStartOf="@+id/background_radio"
            app:layout_constraintTop_toBottomOf="@+id/RSRQ_name"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/SNR_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="34dp"
            android:layout_marginTop="12dp"
            android:text="0"
            android:textColor="#ECE2E2"
            app:layout_constraintBottom_toBottomOf="@+id/background_radio"
            app:layout_constraintEnd_toEndOf="@+id/background_radio"
            app:layout_constraintHorizontal_bias="0.856"
            app:layout_constraintStart_toStartOf="@+id/background_radio"
            app:layout_constraintTop_toBottomOf="@+id/SNR_name"
            app:layout_constraintVertical_bias="0.0" />

        <VideoView
            android:id="@+id/videoView1"
            android:layout_width="157sp"
            android:layout_height="105sp"
            android:layout_marginTop="36dp"
            app:layout_constraintBottom_toTopOf="@+id/background_stream"
            app:layout_constraintEnd_toEndOf="@+id/background_stream"
            app:layout_constraintHorizontal_bias="0.038"
            app:layout_constraintStart_toStartOf="@+id/background_stream"
            app:layout_constraintTop_toBottomOf="@+id/title_stream"
            app:layout_constraintVertical_bias="0.164" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Counter: "
            android:textColor="#ECE2E2"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/background_stream"
            app:layout_constraintHorizontal_bias="0.072"
            app:layout_constraintStart_toEndOf="@+id/videoView1"
            app:layout_constraintTop_toBottomOf="@+id/title_stream" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
